aspect timeslot_http {
	feature HttpMonitoringSystemFeatureModel.HttpMonitoringSystem.QoS.HttpMonitor.LowTimeSlot,
	pointcuts{	
		pointcut Http{
			pattern eu.supersede.dynadapt.usecases.patterns.httpTimeSlot,
			role ADM.Joinpoint
		}
	},
	compositions{
		composition updateTimeSlot{
			feature_enabled true,
			jointpointRole ADM.Joinpoint,
			action update value '30'
		}
	}
}