    <entity name="JavaScript2" class="ptolemy.actor.lib.jjs.JavaScript">
        <property name="script" class="ptolemy.actor.parameters.PortParameter" value="exports.setup = function() {&#10;  this.input('connectionReady');&#10;  this.output('toSend');&#10;}&#10;&#10;var handle;&#10;exports.initialize  = function() {&#10;  handle = this.addInputHandler('connectionReady', handler.bind(this));&#10;}&#10;&#10;function handler() {&#10;  var add = '0123456789';&#10;  var longString = '';&#10;  for (var i = 0; i &lt; 7000; i++) {&#10;    longString += add;&#10;  }&#10;  this.send('toSend', longString);&#10;}&#10; &#10;exports.wrapup = function() {&#10;  this.removeInputHandler(handle);&#10;}">
