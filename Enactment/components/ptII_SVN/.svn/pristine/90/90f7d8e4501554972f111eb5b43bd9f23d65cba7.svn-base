    <entity name="Command" class="ptolemy.actor.lib.jjs.JavaScript">
        <property name="script" class="ptolemy.actor.parameters.PortParameter" value="/** Generate Hue commands.&#10; *&#10; *  @accessor Command&#10; *  @input trigger When an input is received, output a series of Hue commands.&#10; *  @output output A series of Hue commands.&#10; *  @author Elizabeth Osyk&#10; *  @version $$Id$$&#10; */&#10;exports.setup = function() {&#10;    this.input('trigger');&#10;    this.output('output');&#10;    this.parameter('lightID', {'type': 'string'});&#10;};&#10;&#10;exports.initialize = function() {&#10;    var self = this;&#10;    this.addInputHandler('trigger', function() {&#10;    	// On, off, on, off&#10;       var data = [{'id': self.getParameter('lightID'), 'on':true}];&#10;	   self.send('output', data);&#10;	   &#10;	   setTimeout(function() {&#10;	   	data = [{'id':self.getParameter('lightID'), 'on':false}];&#10;	    self.send('output', data);&#10;	    &#10;	    setTimeout(function() {&#10;		   	data = [{'id':self.getParameter('lightID'), 'on':true}];&#10;		    self.send('output', data);&#10;		    &#10;		    setTimeout(function() {&#10;	   			data = [{'id':self.getParameter('lightID'), 'on':false}];&#10;	    		self.send('output', data);&#10;		   	}, 3000);&#10;	    }, 3000);&#10;	   }, 3000);&#10;    });&#10;}&#10;">
