  <entity name="FixedPriorityScheduler" class="ptolemy.actor.lib.aspect.FixedPriorityScheduler"/>
  <entity name="TTESwitch" class="ptolemy.domains.de.lib.aspect.TTESwitch"/>  
   

  


  
  
  
  



