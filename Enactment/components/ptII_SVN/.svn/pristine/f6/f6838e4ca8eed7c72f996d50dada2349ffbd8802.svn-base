#!/bin/sh
# -[Fri Oct 28 12:21:01 2005 by cxh]-
# Return all the text source files (with exceptions) in the ptII tree

$PTII/adm/bin/ptIIfiles | egrep -v '\.#' |egrep -v '\.au$' \
	| egrep -v '\.aif$' \
	| egrep -v '\.bin$' |egrep -v '\.bmp$' \
	| egrep -v '\.class$' |egrep -v '\.dat$' |egrep -v '\.data$' \
	| egrep -v '\.dbxml$' \
	| egrep -v '\.dll$' \
	| egrep -v '\.dll.a$' \
        | egrep -v '\.doc$' \
        | egrep -v '\.dmg$' \
        | egrep -v '\.dSYM/' \
        | egrep -v '\.DS_Store$/' \
	| egrep -v '\.dsp$' |egrep -v '\.dsw$' \
        | egrep -v '\.dylib$' \
        | egrep -v '\.fmu$' \
	| egrep -v '\.fm$'  |egrep -v '\.fig$' \
        | egrep -v '\.ec3$' \
	| egrep -v '\.eps$' |egrep -v '\.exe$' \
        | egrep -v '\.gz$'  |egrep -v '\.gif$' \
        | egrep -v '\.GIF$' \
        | egrep -v '\.git' \
        | egrep -v '\.ico$' |egrep -v '\.icns$' \
        | egrep -v '\.idf$' \
        | egrep -v '\.idl$' \
        | egrep -v '\.ilk$' \
        | egrep -v '\.isj$' \
	| egrep -v '\.jar$' |egrep -v '\.jnilib$' \
        | egrep -v '\.jpg$' \
        | egrep -v '\.JPG$' \
	| egrep -v '\.kar$' \
	| egrep -v 'Keystore$' \
	| egrep -v '.keystore$' \
	| egrep -v 'JavaAppLauncher$' \
	| egrep -v 'JavaApplicationStub$' \
	| egrep -v '/jquery/' \
	| egrep -v 'Localizable.strings$'  \
	| egrep -v '\.lib$'  \
	| egrep -v '\.m$' \
	| egrep -v '\.MF$' \
        | egrep -v '\.mat$' \
	| egrep -v '\.mdl$' |egrep -v '\.mexmaci$' \
	| egrep -v '\.mexa64$' \
	| egrep -v '\.mov$' \
	| egrep -v '\.mtl$' \
        | egrep -v '\.ncb$' \
	| egrep -v '/node_modules/' \
	| egrep -v '\.nqc$' \
        | egrep -v '\.o$' \
        | egrep -v '\.opt$' \
	| egrep -v 'PkgInfo$' \
	| egrep -v '\.png$' \
	| egrep -v '\.PNG$' \
	| egrep -v '\.pom$' \
	| egrep -v '\.pdf$' |egrep -v '\.plt$' \
        | egrep -v '\.pnproj$' | egrep -v '\.pnps$' \
        | egrep -v '\.psd$' |egrep -v '\.ppt$' \
	| egrep -v '\.qcf$' \
        | egrep -v '\.ser$' \
        | egrep -v '\.skp$' \
        | egrep -v '\.so$' \
        | egrep -v '\.so\.[0-9]*$' \
        | egrep -v '\.suo$' \
        | egrep -v '\.tif$' \
        | egrep -v '\.tiff$' \
        | egrep -v '\.vbs$' \
        | egrep -v '\.vsd$' \
	| egrep -v '\.wav$' \
	| egrep -v '\.xls$' \
	| egrep -v '\.zargo$' \
	| egrep -v '\.zip$'
