        <property name="script" class="ptolemy.kernel.util.StringAttribute" value="importPackage(Packages.ptolemy.actor);&#10;importClass(Packages.ptolemy.actor.lib.Ramp);&#10;importClass(Packages.ptolemy.actor.lib.FileWriter);&#10;importClass(Packages.ptolemy.actor.lib.SetVariable);&#10;importClass(Packages.ptolemy.domains.sdf.kernel.SDFDirector);&#10;importClass(Packages.ptolemy.data.expr.Parameter);&#10;&#10;function fire() {&#10;var toplevel = new TypedCompositeActor();&#10;var ramp = new Ramp(toplevel, &quot;ramp&quot;);&#10;var writer = new FileWriter(toplevel, &quot;writer&quot;);&#10;var setVariable = new SetVariable(toplevel, &quot;setVariable&quot;);&#10;setVariable.variableName.setExpression(&quot;count&quot;);&#10;new Parameter(toplevel, &quot;step&quot;);&#10;new Parameter(toplevel, &quot;count&quot;);&#10;ramp.step.setExpression(&quot;step&quot;);&#10;&#10;var relation = toplevel.connect(ramp.output, writer.input);&#10;setVariable.input.link(relation);&#10;&#10;var director = new SDFDirector(toplevel, &quot;SDFDirector&quot;);&#10;director.iterations.setExpression(valueOf(iterations).toString());&#10;&#10;send(toplevel, actor);&#10;}">
