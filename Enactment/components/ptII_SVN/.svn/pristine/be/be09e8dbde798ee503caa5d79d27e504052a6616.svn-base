        <property name="frequency (parameter)" class="ptolemy.kernel.util.StringAttribute" value="The frequency with which to repeat the cycle (in Hz).">
        </property>
        <property name="dutyCycle (port-parameter)" class="ptolemy.kernel.util.StringAttribute" value="The fraction of time to hold the output high.">
