    <entity name="RemoveIPAddress" class="ptolemy.actor.lib.jjs.JavaScript">
        <property name="script" class="ptolemy.actor.parameters.PortParameter" value="/** Remove IP address from response.&#10; *&#10; *  @accessor RemoveIPAddress&#10; *  @input input Remove IP address portion of the response.&#10; *  @output output The HTTP response without the IP address.&#10; *  @author Elizabeth Osyk&#10; *  @version $$Id$$&#10; */&#10;exports.setup = function() {&#10;    this.input('input');&#10;    this.output('output');&#10;};&#10;&#10;exports.initialize = function() {&#10;    var self = this;&#10;    this.addInputHandler('input', function() {&#10;       var data = self.get('input');&#10;       var result = data;&#10;   &#10;       if (data !== null) {&#10;       	  if (typeof data === 'string' || data instanceof String) {&#10;	          // Remove the &quot;origin&quot; (i.e. IP address) since this will differ per machine&#10;	          var start = data.indexOf(&quot;origin&quot;);&#10;	          var end;&#10;	       &#10;	          if (start &gt;= 0) {&#10;	       	     end = data.indexOf(&quot;url&quot;);&#10;	       	     result = data.substring(0, start) + data.substring(end, data.length);&#10;	          }&#10;       	  } else if (typeof data === 'object') {&#10;       	  	if (data.hasOwnProperty('origin')) {&#10;       	  		delete data.origin;&#10;       	  	}&#10;       	  }&#10;       }&#10;	   self.send('output', result);&#10;    });&#10;}&#10;">
