    <property name="DefaultIconScript" class="ptolemy.vergil.basic.export.web.DefaultIconScript" value="writeText('')">
        <property name="displayText" class="ptolemy.data.expr.StringParameter" value="Javascript for icons is entered here.  Right click this text -&gt; Customize -&gt; Configure">
        </property>
        <property name="script" class="ptolemy.data.expr.StringParameter" value="function writeText(text) {&#10;    document.getElementById(&quot;afterImage&quot;).innerHTML = text;&#10;};&#10;&#10;function refresh(period) {&#10;	if (period &gt; 0) {&#10;		setTimeout(&quot;location.reload(true);&quot;, period);&#10;	}&#10;}&#10;&#10;window.onload = refresh($refreshRate)&#10;&#10;&#10;">
        </property>
        <property name="evaluateScript" class="ptolemy.data.expr.Parameter" value="true">
        </property>
        <property name="startText" class="ptolemy.data.expr.StringParameter" value="&lt;div&gt; &#10;&lt;p&gt; This demo is designed to show Sensor Andrew's mobile phone indoor localization capabilities. &lt;/p&gt;&#10;&#10;&lt;p&gt; A map shows the location of up to five devices.  Each mobile device periodically POSTs its location (X, Y coords) and ID number to http://localhost:8074/guidance &lt;/p&gt;&#10;&#10;&lt;p&gt; A GET request to http://localhost:8074/guidance?rate=0 will show the map and all markers. The map will refresh at the given rate (enter 0 if refresh is not desired).  The rate parameter is required. &lt;/p&gt;&#10;&#10;&lt;p&gt; If running without mobile devices, enter a new location in the form fields at the bottom to move a marker. &lt;/p&gt;&#10;&lt;/div&gt;&#10;&#10;&lt;div id=&quot;welcomeMessage&quot;&gt; &lt;/div&gt;">
        </property>
        <property name="endText" class="ptolemy.data.expr.StringParameter" value="&lt;p id=&quot;afterImage&quot;&gt;&lt;/p&gt;&#10;&lt;form accept-charset=&quot;UTF-8&quot; action=&quot;guidance&quot; method=&quot;post&quot;&gt;&#10;	&lt;p&gt; New location? &lt;/p&gt;&#10;	&lt;p&gt; ID (1 to 5):  &lt;input type=&quot;text&quot; name=&quot;ID&quot; id=&quot;ID&quot;/&gt;&#10;	&lt;br&gt; &#10;	X (1 to 500): &lt;input type=&quot;text&quot; name=&quot;xCoord&quot; id=&quot;xCoord&quot;/&gt;&#10;	&lt;br&gt;&#10;	Y (1 to 600): &lt;input type=&quot;text&quot; name=&quot;yCoord&quot; id=&quot;yCoord&quot;/&gt;&#10;	&lt;br&gt;&#10;	&lt;input type=&quot;submit&quot; value=&quot;Submit&quot; /&gt;&#10;	&lt;/p&gt;		&#10;&lt;/form&gt;&#10;">
        </property>
        <property name="jQueryLibraries" class="ptolemy.data.expr.StringParameter" value="&lt;link rel=&quot;stylesheet&quot; type=&quot;text/css&quot; href=&quot;http://ptolemy.org/javascript/jquery.fancybox-1.3.4.css&quot; media=&quot;screen&quot;/&gt;&#10;&lt;script src=&quot;http://code.jquery.com/jquery-1.7.1.min.js&quot;&gt;&lt;/script&gt;&#10;&lt;script type=&quot;text/javascript&quot; src=&quot;http://ptolemy.org/javascript/jquery.fancybox-1.3.4.pack.js&quot;&gt;&lt;/script&gt;&#10;&lt;script type=&quot;text/javascript&quot; src=&quot;http://ptolemy.org/javascript/pt-1.0.0.js&quot;&gt;&lt;/script&gt;&#10;&#10;&#10;&#10;&#10;&#10;	&#10;">
